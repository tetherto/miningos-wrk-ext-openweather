{
  "name": "miningos-weather-worker",
  "version": "0.0.1",
  "description": "MiningOS Weather Worker",
  "keywords": [
    "tether"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/miningos-wrk-ext-openweather.git"
  },
  "engine": {
    "node": ">=20.0"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "brittle tests/unit/*.js",
    "test:coverage": "brittle --coverage tests/{,**/}*.test.js",
    "dev": "DEBUG=* node worker.js --wtype wrk-openweather-rack --env development --rack rack-1"
  },
  "dependencies": {
    "async": "3.2.6",
    "bfx-facs-http": "git+https://github.com/bitfinexcom/bfx-facs-http.git",
    "bfx-facs-interval": "git+https://github.com/bitfinexcom/bfx-facs-interval.git",
    "tether-wrk-base": "git+https://github.com/tetherto/tether-wrk-base.git"
  },
  "devDependencies": {
    "brittle": "3.16.3",
    "debug": "4.4.1",
    "standard": "17.1.2"
  }
}
